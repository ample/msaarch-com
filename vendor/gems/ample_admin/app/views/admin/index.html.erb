<div id="left" class="column span-16 append-1">
	<div class="index_header clearfix">
		<div class="column span-12">
			<% title(model.to_s.pluralize) %>
			<%= content_tag :h2, "Select an Existing #{model}" %>
		</div>
		<div class="column span-4 last">
			<%= link_to_new model, eval("new_#{model.to_s.downcase}_path") %>
		</div>
	</div>
	<ul class="index_list clear sortable">
		<%= render eval("current_#{model.to_s.downcase.pluralize}") %>
	</ul>
	<%= render 'admin/legend' %>
</div>
<div id="right" class="column span-7 last"></div>