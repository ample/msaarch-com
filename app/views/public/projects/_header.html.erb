<section id="alpha" class="clearfix <%= current_market.permalink %>">&nbsp;</section>

<% if current_project.images.size > 1 %>
<section id="controls">
  <%= link_to '<span></span>'.html_safe, '#', :id => 'project-imagery-previous' %>
  <%= link_to '<span></span>'.html_safe, '#', :id => 'project-imagery-next' %>
</section>
<section id="tango">
	<ul id="project-imagery">
	<% current_project.images.each do |image| %>
		<%= content_tag :li, image_asset(image, :dimensions => '600x449#') %>
	<% end %>
	</ul>
</section>
<% end %>