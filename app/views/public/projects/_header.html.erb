<section id="alpha"<%= current_market.background %> class="clearfix <%= current_market.permalink %>">
	<div class="container">
		<div class="span-15 last intro">
			<%= image_asset current_market, :object => current_market.logo, :dimensions => '247x>', :class => 'market-logo', :link => portfolio_path(current_market.permalink) unless current_market.logo.blank? %>
		</div>
	</div>
</section>

<% if current_project.images.size > 1 %>
<section id="controls">
  <%= link_to '<span></span>'.html_safe, '#', :id => 'project-imagery-previous' %>
  <%= link_to '<span></span>'.html_safe, '#', :id => 'project-imagery-next' %>
</section>
<section id="tango">
	<ul id="project-imagery">
	<% current_project.images.each do |image| %>
		<%= content_tag :li, image_asset(image, :dimensions => '600x449#') %>
	<% end %>
	</ul>
</section>
<% end %>