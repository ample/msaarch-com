<li id="<%= dom_id(feature) %>" class="<%= feature.active ? 'active' : 'inactive' %>">
	<div class="manage_association">
		<%= link_to image_tag('admin/delete_btn.gif'),
		    feature_path(feature),
			:class => 'delete_object',
			:method => :delete,
			:confirm => 'Are you sure?',
			:remote => true
		%>
		<%= image_tag 'admin/move_btn.gif', :class => 'handle' %>
	</div> 
	<%= link_to truncate(feature.display_name), [:edit, current_object, feature] %>
</li>
