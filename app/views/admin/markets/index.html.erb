		<div class="column span-11">
			<% title('Projects') %>
			<%= content_tag :h2, 'Select a Market' %>
		</div>
		<div class="column span-5 last">
			<%= link_to_new :market, new_market_path %>
		</div>
	</div>
	<ul class="index_list sortable clear">
		<%= render :partial => 'admin/markets/market', :collection => current_markets %>
	</ul>
	<%= render 'admin/legend' %>
</div>
<div id="right" class="column span-7 last"></div>