		span.wrap {
			display: block;
			margin-top: 25%;
			margin-left: 10%; 
			height: 449px;
			width: 130px; 
			text-indent: -9999em;
			span.bg {
				display: block;
				height: 449px;
				width: 130px; 
				background: url(/assets/elements/arrows.png) no-repeat top left; 
			}
		}
	}
	a.next {
		float: right; 
		span.wrap {
			margin-right: 10%; 
			margin-left: 0; 
			float: right;
			span.bg {
				background-position: top right; 
			}
		}
	}
}


