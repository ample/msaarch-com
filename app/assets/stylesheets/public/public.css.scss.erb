<% Market.live.each do |market| %>
.<%= market.permalink %>, 
.<%= market.permalink %> a {
	color: <%= market.color %>; 
	border-color: <%= market.color %>; 
	border-style: solid;
}
section.<%= market.permalink %> {
	background-color: <%= market.color %>; 
}
<% end %>

@mixin frame {
	border: 3px solid #FFF;
	margin: 0 10px 0 0;
}

html {
	overflow-y: scroll;
	overflow: -moz-scrollbars-vertical;
}

body {
	color: #a1a1a1; 
	background: #171717;
}

hr {
	background: #343434; 
	border: none; 
	border-top: 1px solid #0A0A0A; 
}

ul, ol {
	margin: 0; 
	padding: 0; 
}

a, a:hover {
	color: #FFF;
	text-decoration: none;
}

h1, h2, h3, h4 {
	color: #707070;
	margin: 0;
	padding: 0;
}

h2.section-title, 
h1 {
	font-size: 52px;
	line-height: 50px;
	margin-bottom: 25px;
}

h1.page-title {
	font-family:'HelveticaNeueW01-45Ligh', Arial, sans-serif;
	font-size: 32px; 
	line-height: 38px;
	color: #FFF; 
}

h2 {
	font-family:'HelveticaNeueW01-45Ligh', Arial, sans-serif;
	font-size: 32px;
	line-height: 36px;
	margin-bottom: 20px;
}

h3 {
	font-size: 24px;
	line-height: 32px;
	margin-bottom: 20px;
}

span.overlay,
span.video {
	display: block;
}

span.video {
	border: 3px solid #FFF;
}

span.overlay {
	background: url(/assets/elements/overlay.png) no-repeat center center;
	position: relative;
	float: left;
}

.small span.overlay {
	background-image: url(/assets/elements/overlay-small.png);
}

.medium span.overlay {
	background-image: url(/assets/elements/overlay-medium.png);
}

.frame {
	@include frame; 
}

.left {
	float: left; 
}

.right {
	float: right; 
}

.clr {
	clear: both; 
}

.cj {
	text-align: center; 
}

.rj {
	text-align: right; 
}

.block {
	background: rgba(66,66,66,.20); 
	padding: 20px;
	font-size: 12px; 
	line-height: 16px;
	color: #FFF;
	h2 {
		font-size: 20px; 
		line-height: 26px;
	}
}

div.big {
	background: rgb(32,32,32);
	background: -moz-linear-gradient(top, rgba(32,32,32,1) 0%, rgba(52,52,52,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(32,32,32,1)), color-stop(100%,rgba(52,52,52,1)));
	background: -webkit-linear-gradient(top, rgba(32,32,32,1) 0%,rgba(52,52,52,1) 100%);
	background: -o-linear-gradient(top, rgba(32,32,32,1) 0%,rgba(52,52,52,1) 100%);
	background: -ms-linear-gradient(top, rgba(32,32,32,1) 0%,rgba(52,52,52,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#202020', endColorstr='#343434',GradientType=0 );
	background: linear-gradient(top, rgba(32,32,32,1) 0%,rgba(52,52,52,1) 100%);
	padding-bottom: 15px;
}

.big {
	font-size: 15px;
}


section#header {
	background: #0a0a0a url(/assets/elements/nav-bgx.png) repeat-x bottom left;
	ul {
		margin: 0; 
		padding: 0; 
	}
	li {
		float: right; 
		list-style-type: none;
	}
	li a {
		font-size: 13px;
		display: block; 
		padding: 30px 15px 10px;
		border-left: 1px solid #222121;
		border-right: 1px solid #000;
		color: #7D7D7D;
	}
	li a:hover {
		color: #FFF;
	}
	li.on a {
		color: #FFF; 
		background: #090909;
	}
	li.on li a {
		background: transparent;
	}
	li.drop {
		a {
			padding-right: 25px; 
			background: url(/assets/elements/nav-drop.png) no-repeat right bottom;
		}
		li {
			a {
				padding: 0; 
				background: none;
			}
		}
	}
	li.drop div.subnav {
		position: absolute;
		z-index: 1000;
		display: none;
		background: #1B1B1B;
		width: 140px;
		padding: 10px 15px; 
		li {
			float: none;
			text-align: left;
			margin-bottom: 10px
		}
		a {
			padding: 0;
			border-bottom-width: 2px;
			border-left-width: 0;
			border-right-width: 0;
		}
	}
	img.logo,
	img#logo {
		float: left;
		margin: 20px 15px 0;
	}
	nav {
		float: right; 
		width: 750px; 
	}
}

section#footer {
	font-size: 11px; 
	padding: 20px 0 40px;
	a {
		color: #a1a1a1;
	}
	div#markets {
		a {
			font-weight: bold;
			border-bottom-width: 2px;
		}
		li {
			margin-right: 20px;
			list-style-type: none;
			float: left; 
			strong {
				font-size: 14px;
				color: #FFF;
			}
		}
		p {
			margin-top: 15px;
			clear: left; 
		}
	}
	div#social {
		text-align: right;
		a {
			padding-right: 24px;
			margin-bottom: 2px;
			background: url(/assets/elements/social-icons.gif) no-repeat top right;
			display: block; 
			height: 18px;
		}
		a.linkedin {
			background-position: 100% -20px;
		}
		a.sharethis {
			background-position: 100% -40px;
		}
		span.stButton {
			display: none !important; 
		}
	}
}

